<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.entity.Admin" table="t_Admin">
	
		<id name="id" >
			<column name="id"></column>
			<generator class="native"></generator>
		</id>

		<property name="username">
			<column name="username" not-null="true"></column>
		</property>

		<property name="password" >
			<column name="password" ></column>
		</property>
		 
		<property name="name">
			<column name="name" not-null="true"></column>
		</property>
		 
		<property name="flag" >
			<column name="flag" not-null="true"></column>
		</property> 
		
		<property name="introduction" >
			<column name="introduction" not-null="false"></column>
		</property>
	</class>

</hibernate-mapping>