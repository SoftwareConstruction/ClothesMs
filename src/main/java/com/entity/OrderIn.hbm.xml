<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.entity.OrderIn" table="t_orderIn">
		<id name="orderId">
			<column name="id"></column>
			<generator class="native"></generator>
		</id>

		<property name="docu_number">
			<column name="docu_number" not-null="true"></column>
		</property>

		<property name="flag">
			<column name="flag" not-null="true"></column>
		</property>

		<property name="in_time">
			<column name="in_time" not-null="true"></column>
		</property>

		<property name="source">
			<column name="source" not-null="true"></column>
		</property>
		
		<property name="number">
			<column name="number" not-null="true"></column>
		</property>
		
		
		<many-to-one name="clothes" column=" clothesId" cascade="none"
			class="com.entity.Clothes" not-null="true" lazy="false"></many-to-one>
		<many-to-one name="manager" column="manager" cascade="none"
			class="com.entity.Admin" not-null="true" lazy="false"></many-to-one>
		<many-to-one name="wareHouse" column="warehouseId"
			cascade="none" class="com.entity.WareHouse" not-null="true" lazy="false"></many-to-one>
	</class>
</hibernate-mapping>