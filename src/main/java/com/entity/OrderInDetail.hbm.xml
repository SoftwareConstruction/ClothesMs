<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.entity.OrderInDetail" table="t_orderIn_detail">
		<id name="orderId">
			<column name="id"></column>
			<generator class="native"></generator>
		</id>

		
		<property name="number">
			<column name="number" not-null="true"></column>
		</property>
		
		<many-to-one name="clothes_id" column="clothes_id" cascade="none"
			class="com.entity.Clothes" not-null="true" lazy="false"></many-to-one>
		<many-to-one name="summary_id" column="summary_id"
			cascade="none" class="com.entity.Order" not-null="true" lazy="false"></many-to-one>
	</class>
</hibernate-mapping>